Функция СтруктураДанных(Данные) Экспорт
	
	ст = Новый Структура;
	ст.Вставить("ВидОперации", Данные.ВидОперации);
	ст.Вставить("Группа", Данные.Группа);
	ст.Вставить("Документ", Данные.Документ);
	ст.Вставить("ИНН", Данные.ИНН);
	ст.Вставить("КорСчет", Данные.КорСчет);
	ст.Вставить("Организация", Данные.Организация);
	ст.Вставить("Период", Данные.Период);
	ст.Вставить("СуммаОперации", Данные.СуммаОперации);
	ст.Вставить("Счет", Данные.Счет);
	
	Возврат ст;
	
