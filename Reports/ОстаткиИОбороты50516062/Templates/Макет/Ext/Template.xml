		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Организации.Ссылка КАК Организация,
	СвязьОрганизаций8ИСчетов7.Счет7 КАК Счет7
ПОМЕСТИТЬ ВТОрганизации
ИЗ
	Справочник.Организации КАК Организации
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвязьОрганизаций8ИСчетов7.СрезПоследних(&amp;Период) КАК СвязьОрганизаций8ИСчетов7
		ПО Организации.Ссылка = СвязьОрганизаций8ИСчетов7.Организация8
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Счета7.Ссылка КАК Счет7,
	СвязьОрганизаций8ИСчетов7.Организация8 КАК Организация
ПОМЕСТИТЬ ВТСчета7
ИЗ
	Справочник.Счета7 КАК Счета7
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СвязьОрганизаций8ИСчетов7.СрезПоследних(&amp;Период) КАК СвязьОрганизаций8ИСчетов7
		ПО Счета7.Ссылка = СвязьОрганизаций8ИСчетов7.Счет7
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТОрганизации.Организация КАК Организация,
	ВТОрганизации.Счет7 КАК Счет7,
	Обороты8.Период КАК Период,
	Обороты8.Счет КАК Счет,
	Обороты8.Аналитика КАК Аналитика,
	Обороты8.СуммаНачальныйОстатокДт КАК СуммаНачальныйОстатокДт,
	Обороты8.СуммаНачальныйОстатокКт КАК СуммаНачальныйОстатокКт,
	Обороты8.СуммаОборотДт КАК СуммаОборотДт,
	Обороты8.СуммаОборотКт КАК СуммаОборотКт,
	Обороты8.СуммаКонечныйОстатокДт КАК СуммаКонечныйОстатокДт,
	Обороты8.СуммаКонечныйОстатокКт КАК СуммаКонечныйОстатокКт,
	NULL КАК СуммаНачальныйОстатокДт7,
	NULL КАК СуммаНачальныйОстатокКт7,
	NULL КАК СуммаОборотДт7,
	NULL КАК СуммаОборотКт7,
	NULL КАК СуммаКонечныйОстатокДт7,
	NULL КАК СуммаКонечныйОстатокКт7
ИЗ
	ВТОрганизации КАК ВТОрганизации
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Обороты8 КАК Обороты8
		ПО ВТОрганизации.Организация = Обороты8.Организация
			И (Обороты8.Период = НАЧАЛОПЕРИОДА(&amp;Период, МЕСЯЦ))
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АктуальностьОрганизаций.СрезПоследних(КОНЕЦПЕРИОДА(&amp;Период, МЕСЯЦ), ) КАК АктуальностьОрганизацийСрезПоследних
		ПО (Обороты8.Организация = АктуальностьОрганизацийСрезПоследних.Организация)
ГДЕ
	ЕСТЬNULL(АктуальностьОрганизацийСрезПоследних.Активна, ЛОЖЬ) = ИСТИНА
{ГДЕ
	(ВТОрганизации.Организация В ИЕРАРХИИ (&amp;Организации))}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТСчета7.Организация,
	ВТСчета7.Счет7,
	Обороты7.Период,
	Обороты7.Счет,
	Обороты7.Аналитика,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	Обороты7.СуммаНачальныйОстатокДт,
	Обороты7.СуммаНачальныйОстатокКт,
	Обороты7.СуммаОборотДт,
	Обороты7.СуммаОборотКт,
	Обороты7.СуммаКонечныйОстатокДт,
	Обороты7.СуммаКонечныйОстатокКт
ИЗ
	ВТСчета7 КАК ВТСчета7
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Обороты7 КАК Обороты7
		ПО ВТСчета7.Счет7 = Обороты7.Организация
			И (Обороты7.Период = НАЧАЛОПЕРИОДА(&amp;Период, МЕСЯЦ))
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АктуальностьОрганизаций.СрезПоследних(КОНЕЦПЕРИОДА(&amp;Период, МЕСЯЦ), ) КАК АктуальностьОрганизацийСрезПоследних
		ПО (ВТСчета7.Организация = АктуальностьОрганизацийСрезПоследних.Организация)
ГДЕ
	ЕСТЬNULL(АктуальностьОрганизацийСрезПоследних.Активна, ЛОЖЬ) = ИСТИНА
{ГДЕ
	(ВТСчета7.Организация В ИЕРАРХИИ (&amp;Организации))}</query>
